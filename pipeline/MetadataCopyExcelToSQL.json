{
	"name": "MetadataCopyExcelToSQL",
	"properties": {
		"activities": [
			{
				"name": "Copy Excel to SQL",
				"description": "Speichert Daten von Excel Spreadsheet zu einer vordefinierten SQL Tabelle.",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Create Metadata Table",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.00:10:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBatchTimeout": "00.00:10:00",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Export"
								},
								"sink": {
									"name": "export"
								}
							},
							{
								"source": {
									"name": "Import"
								},
								"sink": {
									"name": "import"
								}
							},
							{
								"source": {
									"name": "Entity Display Name"
								},
								"sink": {
									"name": "entity_display_name"
								}
							},
							{
								"source": {
									"name": "Entity Logical Name"
								},
								"sink": {
									"name": "entity_logical_name"
								}
							},
							{
								"source": {
									"name": "Logical Name"
								},
								"sink": {
									"name": "logical_name"
								}
							},
							{
								"source": {
									"name": "Schema Name"
								},
								"sink": {
									"name": "schema_name"
								}
							},
							{
								"source": {
									"name": "Display Name"
								},
								"sink": {
									"name": "display_name"
								}
							},
							{
								"source": {
									"name": "Attribute Type"
								},
								"sink": {
									"name": "attribute_type"
								}
							},
							{
								"source": {
									"name": "Description"
								},
								"sink": {
									"name": "description"
								}
							},
							{
								"source": {
									"name": "Custom Attribute"
								},
								"sink": {
									"name": "custom_attribute"
								}
							},
							{
								"source": {
									"name": "Type"
								},
								"sink": {
									"name": "type"
								}
							},
							{
								"source": {
									"name": "Additional data"
								},
								"sink": {
									"name": "additional_data"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "metadata_crmikpk_p",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_asql_metadata_crmipk_p",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Create Metadata Table",
				"type": "Script",
				"dependsOn": [
					{
						"activity": "Set table name",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.00:10:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "ls_asql_metadataui",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"parameters": [
								{
									"name": "TableName",
									"type": "String",
									"value": "crmipk_p",
									"direction": "Input"
								}
							],
							"type": "NonQuery",
							"text": "DROP TABLE IF EXISTS dbo.@TableName;\n\nCREATE TABLE @TableName (\n    [export] varchar(5),\n    [import] varchar(5),\n    [entity_display_name] VARCHAR(50) NOT NULL,\n    [entity_logical_name] VARCHAR(50) NOT NULL,\n    [logical_name] VARCHAR(50) NOT NULL,\n    [schema_name] VARCHAR(50) NOT NULL,\n    [display_name] VARCHAR(100),\n    [attribute_type] VARCHAR(20) NOT NULL,\n    [description] VARCHAR(1000),\n    [custom_attribute] VARCHAR(5) NOT NULL,\n    [type] VARCHAR(10),\n    [additional_data] VARCHAR(1000)\n);"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
			{
				"name": "Set table name",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "v_tablename",
					"value": "crmipk_p"
				}
			}
		],
		"variables": {
			"v_tablename": {
				"type": "String"
			}
		},
		"annotations": []
	}
}
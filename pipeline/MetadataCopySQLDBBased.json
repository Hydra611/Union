{
	"name": "MetadataCopySQLDBBased",
	"properties": {
		"activities": [
			{
				"name": "Get Suffixes",
				"type": "Script",
				"dependsOn": [
					{
						"activity": "Test Copy Data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "AzureSqlDatabase1",
					"type": "LinkedServiceReference",
					"parameters": {
						"db": {
							"value": "@pipeline().parameters.p_db",
							"type": "Expression"
						}
					}
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "SELECT concat('Names_',Namesuffix) as Tablename\nFROM UiConfig.Suffixes \nAS [result]"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
			{
				"name": "Loop Over Suffixes",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Set Variable",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('v_suffix')",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Change Names to Capital",
							"type": "Script",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"linkedServiceName": {
								"referenceName": "AzureSqlDatabase1",
								"type": "LinkedServiceReference",
								"parameters": {
									"db": {
										"value": "@pipeline().parameters.p_db",
										"type": "Expression"
									}
								}
							},
							"typeProperties": {
								"scripts": [
									{
										"type": "NonQuery",
										"text": {
											"value": "TRUNCATE TABLE UiLoad.@{item().Tablename};\n\nINSERT INTO UiLoad.@{item().Tablename} (Name, Age)\nSELECT upper(Name) AS Name,\n    Age\nFROM UiStaging.@{item().Tablename};",
											"type": "Expression"
										}
									}
								],
								"scriptBlockExecutionTimeout": "02:00:00"
							}
						}
					]
				}
			},
			{
				"name": "Set Variable",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Get Suffixes",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "v_suffix",
					"value": {
						"value": "@activity('Get Suffixes').output.resultSets[0].rows",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Test Copy Data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT upper(Name) AS Name,\n    Age;",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Stage_Names_A",
						"type": "DatasetReference",
						"parameters": {
							"c_db": {
								"value": "@pipeline().parameters.p_db",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Load_Names_A",
						"type": "DatasetReference",
						"parameters": {
							"c_db": {
								"value": "@pipeline().parameters.p_db",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"p_db": {
				"type": "string",
				"defaultValue": "dev-transformed-sqldb"
			}
		},
		"variables": {
			"v_suffix": {
				"type": "Array"
			},
			"v_tablename": {
				"type": "String"
			}
		},
		"annotations": []
	}
}